<table id="example2" class="table table-bordered table-hover">
  <thead>
  <tr>
    <th>No</th>
    <th>Nama Koin</th>
    <th>Jumlah Koin</th>
    <th>Harga Koin</th>
    <th>#</th>
  </tr>
  </thead>
  <tbody>
    <% for(let i = 0; i < nominals.length; i++) { %>
      <tr>
        <td><%= i+1 %></td>
        <td><%= nominals[i].coinName %></td>
        <td><%= nominals[i].coinQuantity %></td>
        <td><%= nominals[i].price %></td>
        <td>
          <form action="/nominal/delete/<%= nominals[i]._id %>?_method=DELETE" method="POST">
            <a href="/nominal/edit/<%= nominals[i]._id %>" class="btn btn-sm btn-warning">
              <i class="fas fa-pencil-alt"></i> Ubah
            </a>
            <button class="btn btn-sm btn-danger" type="submit">
              <i class="fas fa-trash-alt"></i> Hapus
            </button>
          </form>
        </td>
      </tr>
    <% } %>
  </tbody>
</table>